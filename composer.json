{
    "name": "dcarbone/amber-hat",
    "type": "library",
    "description": "Lib designed to help PHP apps consume from REDCap's API",
    "homepage": "https://github.com/dcarbone/amber-hat",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Daniel Carbone",
            "email": "daniel.p.carbone@gmail.com"
        }
    ],
    "keywords": [
        "redcap",
        "red cap"
    ],

    "require": {
        "php": ">=5.3.3",
        "ext-libxml": "*",
        "lib-libxml": "*",
        "ext-curl": "*",
        "lib-curl": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "lib-pcre": "*",
        "ext-date": "*",
        "ext-SPL": "*",
        "dcarbone/curl-plus": "2.0.*"
    },

    "suggest": {
        "ext-openssl": "For that SSL goodness.",
        "lib-openssl": ""
    },

    "autoload": {
        "psr-4": {
            "DCarbone\\AmberHat\\": "src/"
        }
    }
}